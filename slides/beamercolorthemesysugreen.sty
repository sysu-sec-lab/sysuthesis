\setbeamercolor*{palette primary}{fg=sysugreen!60!black,bg=gray!30!white}
\setbeamercolor*{palette secondary}{fg=sysugreen!70!black,bg=gray!15!white}
\setbeamercolor*{palette tertiary}{bg=sysugreen!80!black,fg=gray!10!white}
\setbeamercolor*{palette quaternary}{fg=sysugreen,bg=gray!5!white}

\setbeamercolor*{sidebar}{fg=sysugreen,bg=gray!15!white}

\setbeamercolor*{palette sidebar primary}{fg=sysugreen!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=sysugreen!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=gray!10!white}

%\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{titlelike}{parent=palette primary,fg=sysugreen}
\setbeamercolor{frametitle}{bg=gray!10!white}
\setbeamercolor{frametitle right}{bg=gray!60!white}

